{
  "name": "nestjs-endpoint-finder",
  "displayName": "NestJS Endpoint Finder",
  "description": "Find and jump to NestJS endpoints quickly",
  "version": "0.0.1",
  "publisher": "yaquawa",
  "icon": "resources/icons/nextjs-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/yaquawa/nestjs-endpoint-finder.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "NestJS Endpoint Finder",
      "properties": {
        "nestjsEndpoints.filePatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "**/*.controller.ts"
          ],
          "description": "File patterns to search for NestJS controllers and services. Supports glob patterns.",
          "scope": "workspace"
        },
        "nestjsEndpoints.excludePatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "**/node_modules/**",
            "**/dist/**",
            "**/.git/**",
            "**/coverage/**"
          ],
          "description": "File patterns to exclude from search. Supports glob patterns.",
          "scope": "workspace"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "nestjsEndpointFinder",
          "title": "NestJS Endpoints",
          "icon": "./resources/icons/nextjs-icon.svg"
        }
      ]
    },
    "views": {
      "nestjsEndpointFinder": [
        {
          "id": "nestjsEndpoints",
          "name": "NestJS Endpoints",
          "type": "webview",
          "when": "true"
        }
      ]
    },
    "commands": [
      {
        "command": "nestjsEndpoints.search",
        "title": "Search NestJS Endpoints",
        "icon": "$(search)",
        "category": "NestJS"
      }
    ],
    "menus": {},
    "keybindings": [
      {
        "command": "nestjsEndpoints.search",
        "key": "ctrl+shift+\\",
        "mac": "ctrl+shift+\\",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run lint && npm run compile && npm run build:webview",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build:webview": "vite build",
    "dev:webview": "vite",
    "preview:webview": "vite preview",
    "postinstall": "node scripts/patch-fzf.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.17.58",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.5.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "fzf": "^0.5.2",
    "typescript": "^5.8.0"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
